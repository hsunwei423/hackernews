{
  "name": "hackernews",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "cross-env NODE_ENV=production && next build",
    "start": "next start",
    "export": "next export",
    "deploy": "rm -rf node_modules/.cache && next build && next export && touch out/.nojekyll && git add out/ && git commit -m \"Deploy Next.js to gh-pages\" && git subtree push --prefix out origin gh-pages"
  },
  "dependencies": {
    "antd": "^4.6.6",
    "axios": "^0.20.0",
    "next": "latest",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "sass": "^1.26.11"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^14.11.2",
    "@types/react": "^16.9.49",
    "babel-plugin-transform-define": "^1.3.0",
    "cross-env": "^7.0.2",
    "typescript": "^4.0.3"
  }
}
